<div class="page-header">
  <h1>New Admin</h1>
</div>
<%= form_for @admin do |f| %>
    <div class="form-group">
      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control' %>
    </div>

    <div class="form-group actions">
      <%= f.submit 'Save', class: 'btn btn-primary' %>
    </div>
<% end %>